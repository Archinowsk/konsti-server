{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "app.ts",
  "scripts": {
    "build": "npm run build-js",
    "build-front-dev": "cd ../konsti-client && npm run build-dev && cd ../konsti-server && npm run copy-front",
    "build-front-prod": "cd ../konsti-client && npm run build && cd ../konsti-server && npm run copy-front",
    "build-front-staging": "cd ../konsti-client && npm run build-staging && cd ../konsti-server && npm run copy-front",
    "build-js": "babel src -d lib --extensions \".ts\"",
    "build-types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "copy-front": "rimraf front && ncp ../konsti-client/build ./front",
    "deploy-prod": "npm run prepare-deploy-prod && eb deploy konsti-prod",
    "deploy-staging": "npm run prepare-deploy-staging && eb deploy konsti-staging",
    "docker-start": "docker-compose -f docker-compose.yml up --build",
    "eslint": "eslint . --ext .js,.ts",
    "eslint-rules": "eslint --print-config .eslintrc.js",
    "fixer": "npm run build && node lib/statistics/dataFixer.js --",
    "generate-data": "npm run build && node lib/test/test-data-generation/runGenerators.js",
    "generate-serials": "npm run build && node lib/utils/generateSerials.js",
    "lint": "npm run eslint && npm run prettier:check",
    "prepare-deploy-prod": "npm run build-front-prod && npm run build",
    "prepare-deploy-staging": "npm run build-front-staging && npm run build",
    "prettier:check": "prettier --check ./**/*.{js,json,md,ts,yml}",
    "prettier:write": "prettier --write ./**/*.{js,json,md,ts,yml}",
    "remove-invalid-games": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/removeInvalidGames.js",
    "start": "npm run build && node lib/app.js",
    "start-dev": "npm run build && cross-env NODE_ENV=development nodemon --ext js,ts,json --exec node lib/app.js",
    "stats": "npm run build && node lib/statistics/statGenerator.js --",
    "test": "jest",
    "test-assign": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/testAssignPlayers.js",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "update-game-popularity": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/testUpdateGamePopularity.js",
    "verify-results": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/testVerifyResults.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint",
      "prettier --check"
    ],
    "*.{json,md,yml}": [
      "prettier --check"
    ]
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.2.6"
    }
  },
  "dependencies": {
    "array-flat-polyfill": "1.0.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "commander": "5.1.0",
    "eventassigner-js": "0.1.6",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "express-static-gzip": "2.0.6",
    "express-validator": "6.5.0",
    "generate-serial-number": "0.0.3",
    "helmet": "3.22.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "2.26.0",
    "mongoose": "5.9.15",
    "morgan": "1.10.0",
    "munkres-js": "1.2.2",
    "node-schedule": "1.3.2",
    "npm-check-updates": "6.0.1",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "rimraf": "3.0.2",
    "runtypes": "4.2.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.9.0",
    "@types/bcryptjs": "2.4.2",
    "@types/body-parser": "1.19.0",
    "@types/eslint": "6.8.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/express": "4.17.6",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "4.1.12",
    "@types/helmet": "0.0.47",
    "@types/jest": "25.2.3",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.152",
    "@types/mongoose": "5.7.21",
    "@types/morgan": "1.9.0",
    "@types/ncp": "2.0.4",
    "@types/node": "14.0.5",
    "@types/node-schedule": "1.3.0",
    "@types/nodemon": "1.19.0",
    "@types/prettier": "2.0.1",
    "@types/request": "2.48.5",
    "@types/request-promise-native": "1.0.17",
    "@types/rimraf": "3.0.0",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard-with-typescript": "16.0.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "19.0.1",
    "faker": "4.1.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.6",
    "mongodb-memory-server": "6.6.1",
    "ncp": "2.0.0",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "typescript": "3.9.3"
  },
  "engines": {
    "node": ">=12.x.x"
  }
}
