{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "scripts": {
    "build-front-dev": "cd ../konsti-client && npm run build-dev && cd ../konsti-server && npm run copy-front",
    "build-front-prod": "cd ../konsti-client && npm run build && cd ../konsti-server && npm run copy-front",
    "build-front-staging": "cd ../konsti-client && npm run build-staging && cd ../konsti-server && npm run copy-front",
    "build": "babel src -d lib",
    "clean": "rimraf lib",
    "copy-front": "rimraf front && ncp ../konsti-client/build ./front",
    "deploy-prod": "npm run prepare-deploy-prod && eb deploy Konsti-env",
    "deploy-staging": "npm run prepare-deploy-staging && eb deploy konsti-staging-env",
    "docker-start": "docker-compose -f docker-compose.yml up --build",
    "eslint-rules-table": "eslint-index .eslintrc.js --format table",
    "eslint-rules": "eslint-index .eslintrc.js",
    "eslint": "eslint src --fix",
    "flow-coverage-report": "flow-coverage-report -c 10 -i 'src/**/*.js'",
    "flow-typed-install": "rimraf flow-typed && flow-typed install --skip --ignoreDeps peer bundle",
    "flow": "flow check",
    "generate-data": "babel-node src/test/data-generation/runGenerators.js",
    "generate-serials": "babel-node src/utils/generateSerials.js",
    "prepare-deploy-prod": "npm run clean && npm run build-front-prod && npm run build",
    "prepare-deploy-staging": "npm run clean && npm run build-front-staging && npm run build",
    "prettier": "prettier --write src/**/*.{js,json,md}",
    "start-dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/server.js",
    "start": "npm run build && node lib/server.js",
    "stats:anonymize": "babel-node src/statistics/dataAnonymizer.js",
    "stats:fix": "babel-node src/statistics/dataFixer.js",
    "stats:games": "babel-node src/statistics/getGameStats.js",
    "stats:results": "babel-node src/statistics/getResultsStats.js",
    "stats:users": "babel-node src/statistics/getUserStats.js",
    "test-assign": "cross-env NODE_ENV=development babel-node src/test/testAssignPlayers.js",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.0",
    "@babel/node": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-plugin-module-resolver": "3.2.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "express-static-gzip": "1.1.3",
    "generate-serial-number": "0.0.3",
    "helmet": "3.18.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "mongoose": "5.6.3",
    "morgan": "1.9.1",
    "munkres-js": "1.2.2",
    "node-schedule": "1.3.2",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "rimraf": "2.6.3",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.9.0"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-lodash": "3.3.4",
    "cross-env": "5.2.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-index": "1.5.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-eslint-plugin": "2.1.0",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "9.1.1",
    "faker": "4.1.0",
    "flow-bin": "0.102.0",
    "flow-coverage-report": "0.6.1",
    "flow-typed": "2.5.2",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.1.0",
    "ncp": "2.0.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "supertest": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  }
}
