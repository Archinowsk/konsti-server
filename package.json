{
  "name": "ropecon-rpg-signup",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Ropecon RPG Signup",
  "author": {
    "name": "Arttu Hanska"
  },
  "engines": {
    "node": "8.1.x"
  },
  "scripts": {
    "start": "node src/server.js",
    "start-dev": "cross-env NODE_ENV=development node src/server.js",
    "start-prod": "cross-env NODE_ENV=production node src/server.js",
    "test": "mocha --reporter spec --compilers js:babel-core/register",
    "eslint": "eslint --ext .js,.jsx src",
    "eslint-fix": "eslint  --fix --ext .js,.jsx src",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src}/**/*.jsx\"",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "generate-data": "cross-env NODE_ENV=development node src/test/data-generation/runGenerators.js",
    "generate-serials": "node -e \"require('./src/utils/serials.js').generateSerials();\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "generate-serial-number": "0.0.3",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "i18next-localstorage-cache": "^1.1.1",
    "i18next-xhr-backend": "^1.4.2",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.4",
    "morgan": "^1.8.2",
    "munkres-js": "^1.2.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "cross-env": "^5.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "prettier": "^1.5.3"
  }
}
