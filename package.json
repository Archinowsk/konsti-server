{
  "name": "ropecon-rpg-signup",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Ropecon RPG Signup",
  "author": {
    "name": "Arttu Hanska"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "scripts": {
    "start": "node src/server.js",
    "start-dev": "cross-env NODE_ENV=development node src/server.js",
    "start-prod": "cross-env NODE_ENV=production node src/server.js",
    "start-test": "cross-env NODE_ENV=testing node src/server.js",
    "test": "mocha --reporter spec --compilers js:babel-core/register",
    "eslint": "eslint src --fix",
    "prettier": "prettier --write \"src/**/*.{js,json}\"",
    "generate-data": "cross-env NODE_ENV=development node src/test/data-generation/runGenerators.js",
    "generate-serials": "node -e \"require('./src/utils/serials.js').generateSerials();\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "generate-serial-number": "0.0.3",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "i18next-localstorage-cache": "^1.1.1",
    "i18next-xhr-backend": "^1.4.2",
    "jsonwebtoken": "^8.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "mongoose": "^4.11.7",
    "morgan": "^1.8.2",
    "munkres-js": "^1.2.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run eslint",
      "npm run prettier",
      "git add"
    ],
    "*.json": [
      "npm run prettier",
      "git add"
    ]
  }
}
