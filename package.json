{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "scripts": {
    "build": "rimraf lib && babel src -d lib",
    "build-front": "cd ../konsti-client && npm run build",
    "copy-front": "rimraf front && ncp ../konsti-client/build ./front",
    "deploy": "npm run prepare-deploy && eb deploy",
    "docker-start": "docker-compose -f docker-compose.yml up --build",
    "eslint": "eslint src --fix",
    "flow": "flow check",
    "flow-typed-install": "flow-typed install --skip --overwrite --ignoreDeps peer bundle",
    "generate-data": "cross-env NODE_ENV=development babel-node src/test/data-generation/runGenerators.js",
    "generate-serials": "npm run build && node -r esm --eval \"require('./lib/utils/serials.js').generateSerials()\"",
    "precommit": "lint-staged",
    "prepare-deploy": "npm run build-front && npm run copy-front && npm run build",
    "prettier": "prettier --write \"src/**/*.{js,json}\"",
    "start": "npm run build && node -r esm lib/server.js",
    "start-dev": "npm run build && cross-env NODE_ENV=development node -r esm lib/server.js",
    "start-prod": "npm run build && cross-env NODE_ENV=production node -r esm lib/server.js",
    "stats:games": "babel-node src/statistics/getGameStats.js",
    "stats:results": "babel-node src/statistics/getResultsStats.js",
    "stats:users": "babel-node src/statistics/getUserStats.js",
    "test": "mocha --reporter spec --compilers js:babel-core/register",
    "test-assign": "cross-env NODE_ENV=development babel-node src/test/testAssignPlayers.js"
  },
  "dependencies": {
    "@babel/cli": "7.0.0-beta.53",
    "@babel/core": "7.0.0-beta.53",
    "@babel/node": "7.0.0-beta.53",
    "@babel/preset-env": "7.0.0-beta.53",
    "@babel/preset-flow": "7.0.0-beta.53",
    "babel-plugin-module-resolver": "3.1.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "esm": "3.0.66",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "generate-serial-number": "0.0.3",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "mongoose": "5.2.3",
    "morgan": "1.9.0",
    "munkres-js": "1.2.2",
    "request": "2.87.0",
    "request-promise-native": "1.0.5",
    "rimraf": "2.6.2",
    "winston": "3.0.0",
    "winston-daily-rotate-file": "3.2.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.5",
    "cross-env": "5.2.0",
    "eslint": "5.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "faker": "4.1.0",
    "flow-bin": "0.76.0",
    "flow-typed": "2.5.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "ncp": "2.0.0",
    "prettier": "1.13.7"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --parser flow --write",
      "git add"
    ],
    "*.json": [
      "prettier --parser json --write",
      "git add"
    ]
  }
}
