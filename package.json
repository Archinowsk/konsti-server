{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "main": "app.ts",
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "engines": {
    "node": ">=12.x.x"
  },
  "scripts": {
    "build-front-dev": "cd ../konsti-client && npm run build-dev && cd ../konsti-server && npm run copy-front",
    "build-front-prod": "cd ../konsti-client && npm run build && cd ../konsti-server && npm run copy-front",
    "build-front-staging": "cd ../konsti-client && npm run build-staging && cd ../konsti-server && npm run copy-front",
    "build": "npm run clean && npm run build-types && npm run build-js",
    "build-types": "tsc --emitDeclarationOnly",
    "build-js": "babel src -d lib --extensions \".ts,.tsx\"",
    "clean": "rimraf lib",
    "copy-front": "rimraf front && ncp ../konsti-client/build ./front",
    "deploy-prod": "npm run prepare-deploy-prod && eb deploy konsti-prod",
    "deploy-staging": "npm run prepare-deploy-staging && eb deploy konsti-staging",
    "docker-start": "docker-compose -f docker-compose.yml up --build",
    "eslint-rules-table": "eslint-index .eslintrc.js --format table",
    "eslint-rules": "eslint-index .eslintrc.js",
    "eslint": "eslint . --ext .js,.ts --fix",
    "generate-data": "npm run build && node lib/test/test-data-generation/runGenerators.js",
    "generate-serials": "npm run build && node lib/utils/generateSerials.js",
    "prepare-deploy-prod": "npm run clean && npm run build-front-prod && npm run build",
    "prepare-deploy-staging": "npm run clean && npm run build-front-staging && npm run build",
    "prettier": "prettier --write ./**/*.{js,json,md,ts,yml}",
    "start-dev": "npm run build && cross-env NODE_ENV=development nodemon --exec node lib/app.js",
    "start": "npm run build && node lib/app.js",
    "fixer": "npm run build && node lib/statistics/dataFixer.js --",
    "stats": "npm run build && node lib/statistics/statGenerator.js --",
    "test-assign": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/testAssignPlayers.js",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "test": "jest",
    "update-game-popularity": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/testUpdateGamePopularity.js",
    "verify-results": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/testVerifyResults.js",
    "remove-invalid-games": "npm run build && cross-env NODE_ENV=development node lib/test/scripts/removeInvalidGames.js",
    "lint": "eslint . && prettier --check ./**/*.{js,ts,json,md}",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/preset-env": "7.8.4",
    "array-flat-polyfill": "1.0.1",
    "await-to-js": "2.1.1",
    "babel-plugin-module-resolver": "4.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "commander": "4.1.0",
    "eventassigner-js": "0.1.6",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "express-static-gzip": "2.0.6",
    "express-validator": "6.4.0",
    "generate-serial-number": "0.0.3",
    "helmet": "3.21.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mongoose": "5.8.11",
    "morgan": "1.9.1",
    "munkres-js": "1.2.2",
    "node-schedule": "1.3.2",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "rimraf": "3.0.1",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-plugin-lodash": "3.3.4",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard-with-typescript": "12.0.1",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-index": "1.5.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "16.0.0",
    "faker": "4.1.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "mongodb-memory-server": "6.2.4",
    "ncp": "2.0.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.2.3"
    }
  }
}
